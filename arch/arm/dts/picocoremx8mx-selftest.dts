	in-pins = "SD_A_CLK", "SD_A_RST", "SD_A_WP", "SD_A_DATA1", "SD_A_DATA3", "SD_A_DATA5", "SD_A_DATA7";
	in-gpios = <&gpio2 0 0>, <&gpio2 10 0>, <&gpio2 20 0>, <&gpio2 3 0>, <&gpio2 5 0>, <&gpio2 7 0>, <&gpio2 9 0>;
